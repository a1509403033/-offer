class Solution {
public:
    char findTheDifference(string s, string t) 
    {
        sort(s.begin(), s.end());
        sort(t.begin(), t.end());
        int i, len = s.size();
        for(i = 0; i < len; i++){
            if(s[i] != t[i])
            {
                return t[i];
            }
        }
        return t[i];   
    }
};
